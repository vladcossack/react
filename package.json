    "code:check": "./node_modules/.bin/eslint '**/*.js'",
    "lint": "eslint ./src --ext .js,.jsx",
    "code:fix": "./node_modules/.bin/eslint '**/*.js' --fix",
    "start:db": "json-server --watch db.json -p 3333",
    "clean": "rm -rf node_modules/ && npm i"
  },
